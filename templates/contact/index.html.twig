{% extends 'base.html.twig' %}

{% block title %} {{ parent() }} - Contact{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('contactCss') }}
    {{ encore_entry_link_tags('blurCss') }}
{% endblock %}

{% block body %}

<main class="container">

    {{ form_start(form,{'attr': {'class': 'bg-form'}}) }}
    <p class="head_form">Un question, une suggestion? Contactez-nous!</p>
    {{ form_widget(form) }}
    <button type="submit" class="submit">Envoyer</button>
    {{ form_end(form) }}
</main>
{% endblock %}
